<!-- 头部 -->
<nav class="navbar navbar-default">
    <div class="container">
        <div class="container-fluid">
            <!-- 移动端显示 -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#menus-navbar" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#"><%= title %></a>
            </div>
            <div class="collapse navbar-collapse" id="menus-navbar">
                <% if(types.length > 0) { %>
                    <ul class="nav navbar-nav">
                        <% if(current == 'index'){ %>
                            <li class="active"><a href="/">首页<span class="sr-only">(current)</span></a></li> 
                        <% } else { %>
                         <li><a href="/">首页</a></li> 
                        <% } %>
                        <% types.forEach(function(item){ %>
                            <% if(current == item.typeId ){ %>
                                <li class="active"><a href="/list/<%= item.typeId %>"><%= item.typeName %><span class="sr-only">(current)</span></a></li>
                            <% } else { %>
                                <li><a href="/list/<%= item.typeId %>"><%= item.typeName %></a></li> 
                            <% } %>
                        <% }) %>
                    </ul>
                <% } %>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="javascript:void(0)" class='login-btn'>登录</a></li>
                    <li><a href="javascript:void(0)" class='register'>免费注册</a></li>
                </ul>
            </div>
        </div>
    </div>
</nav>
<div class='cover'></div>
<%- include('login') %>